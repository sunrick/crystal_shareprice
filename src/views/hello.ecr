<html>
  <head>
    <script   src="https://code.jquery.com/jquery-2.2.2.min.js"   integrity="sha256-36cp2Co+/62rEAAYHLmRCPIych47CvdM+uTBJwSzWjI="   crossorigin="anonymous"></script>
      <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">

    <!-- Latest compiled and minified JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>
    <script src="//www.google.com/jsapi"></script>
    <script src="/js/chartkick.js"></script>
  </head>
  <body>
    <div class="container">

      <div class="row">
        <div class="col-xs-4">
          <h1> <%= ticker %> </h1>
        </div>
        <div class="col-xs-8">
          <form action="/company" method="get" class="form-inline pull-right" style="margin-top: 20px">
            <div class="form-group">
              <label for="ticker"> Ticker </label>
              <input type="text" class="form-control" name="ticker" id="ticker">
            </div>
            <button type="submit" class="btn btn-default">Submit</button>
          </form>
        </div>
      </div>

      <div class="row">
        <div class="col-xs-12">
          
          <div id="chart-1" style="height: 300px;"></div>
        </div>
      </div>
    </div>



    <% if share_prices %>
      <script>new Chartkick.LineChart("chart-1", <%= share_prices %>, {"adapter": "google", "library": { "pointsVisible": false }} );</script>
    <% end %>
  </body>

</html>